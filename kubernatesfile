apiVersion: apps/v1
kind: Deployment
metadata:
  name: hemanth108/weapps:tagname
  labels:
    app: webapp
spec:
  replicas: 3
  selector:
    matchLabels:
      app: webapp
  template:
    metadata:
      labels:
        app: webapp
    spec:
      containers:
      - name: webapp
        image: hemanth108/weapps:tagname
        ports:
        - containerPort: 80
